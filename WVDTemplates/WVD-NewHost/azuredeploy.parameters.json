{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "AdminUserName": {
      "value": "DOMAIN USER NAME"
    },
    "AdminPassword": {
      "value": "<DOMAIN USER PASSWORD>"
    },
    "DomainFQDN": {
      "value": "FULL QUALIFIED DOMAIN i.e. MSAzureAcademy.com"
    },
"subnetName": {
      "value": "<SUBNET NAME>"
    },
    "virtualNetworkId": {
      "value": "/subscriptions/<SUBSCRIPTION ID>/resourceGroups/<RESOURCE GROUP NAME>/providers/Microsoft.Network/virtualNetworks/<VIRTUAL NETWORK NAME>"
    },
    "virtualMachineName": {
      "value": "<ENTER VM NAME>"
    }
  }
}